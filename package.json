{
  "name": "woodenlist",
  "version": "0.1.0",
  "description": "A realtime todolist",
  "main": "index.js",
  "scripts": {
    "watch": "shadow-cljs watch client server",
    "build": "shadow-cljs clj-run build.main/build",
    "build-local": "shadow-cljs clj-run build.main/build-local",
    "page": "shadow-cljs clj-run build.main/page",
    "upload": "lumo -c cli -m build.upload",
    "serve": "http-server dist -s",
    "repl": "rlwrap shadow-cljs clj-repl",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TopixIM/woodenlist.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.7.7",
    "md5": "^2.2.1",
    "randomcolor": "^0.5.3",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.7.6",
    "source-map-support": "^0.5.9",
    "ws": "^6.1.1"
  }
}
